{
  "name": "expense-management-system",
  "version": "1.0.0",
  "description": "Complete expense management platform for teams and companies",
  "main": "index.js",
  "scripts": {
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "start:full": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "build:frontend": "cd frontend && npm run build",
    "lint:frontend": "cd frontend && npm run lint",
    "demo": "cd backend && npm run demo"
  },
  "keywords": [
    "expense-management",
    "react",
    "nextjs",
    "nodejs",
    "express",
    "mysql",
    "typescript"
  ],
  "author": "Expense Management Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "react-chartjs-2": "^5.3.0"
  }
}
